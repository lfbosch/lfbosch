@article{dana2007,
	title = {Exploiting moral wiggle room: experiments demonstrating an illusory preference for fairness},
	volume = {33},
	issn = {0938-2259, 1432-0479},
	shorttitle = {Exploiting moral wiggle room},
	url = {http://link.springer.com/10.1007/s00199-006-0153-z},
	doi = {10.1007/s00199-006-0153-z},
	abstract = {This paper explores whether generosity in experiments is truly evidence of concern for desirable social outcomes. We conduct an experiment using a binary version of the dictator game. We introduce several treatments in which subjects are able to leave the relationship between their actions and resulting outcomes uncertain, either to themselves or to another subject inﬂuenced by those actions, thus giving subjects the moral “wiggle room” to behave self-interestedly. We ﬁnd signiﬁcantly less generous behavior in these manipulations, relative to a baseline in which the relationship between actions and outcomes is transparent. We conclude that many subjects behave fairly in the baseline case mainly because they intrinsically dislike appearing unfair, either to themselves or others.},
	language = {en},
	number = {1},
	urldate = {2018-10-13},
	journal = {Economic Theory},
	author = {Dana, Jason and Weber, Roberto A. and Kuang, Jason Xi},
	month = jul,
	year = {2007},
	pages = {67--80},
	file = {Dana et al. - 2007 - Exploiting moral wiggle room experiments demonstr.pdf:/Users/lukasbosch/Zotero/storage/DZ5V894V/Dana et al. - 2007 - Exploiting moral wiggle room experiments demonstr.pdf:application/pdf}
}

@article{hilbig_what_2013,
	title = {What lies beneath: {How} the distance between truth and lie drives dishonesty},
	volume = {49},
	issn = {00221031},
	shorttitle = {What lies beneath},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S002210311200234X},
	doi = {10.1016/j.jesp.2012.11.010},
	abstract = {Based on the assumption that dishonesty poses a threat to one's self view, recent research has put forward the notion that people avoid major lies. However, existing empirical work has not tested this notion conclusively, given that studies have associated larger degrees of dishonesty with larger payoffs. It thus remains unclear whether people actually do avoid major lies or rather shy away from large (unjustiﬁed) payoffs, e.g. since the latter are generally more likely to trigger suspicion. Thus, we critically tested the hypothesis that the probability of dishonesty is a decreasing function of the distance between the actual truth and the lie that is necessary to increase ones gains. In a modiﬁed dice-game paradigm, a highly speciﬁc behavioral pattern was predicted by this hypothesis and results from a large (N = 765), incentivized and fully anonymous study conﬁrmed the latter, thus corroborating that people indeed avoid major lies.},
	language = {en},
	number = {2},
	urldate = {2018-10-13},
	journal = {Journal of Experimental Social Psychology},
	author = {Hilbig, Benjamin E. and Hessler, Corinna M.},
	month = mar,
	year = {2013},
	pages = {263--266},
	file = {Hilbig and Hessler - 2013 - What lies beneath How the distance between truth .pdf:/Users/lukasbosch/Zotero/storage/RTTT9AUZ/Hilbig and Hessler - 2013 - What lies beneath How the distance between truth .pdf:application/pdf}
}

