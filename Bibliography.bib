@article{jacobsen2018,
	title = {{WHY} {DO} {WE} {LIE}? {A} {PRACTICAL} {GUIDE} {TO} {THE} {DISHONESTY} {LITERATURE}: {WHY} {DO} {WE} {LIE}?},
	volume = {32},
	issn = {09500804},
	shorttitle = {{WHY} {DO} {WE} {LIE}?},
	url = {http://doi.wiley.com/10.1111/joes.12204},
	doi = {10.1111/joes.12204},
	abstract = {Over the last decade, a massive body of research has been devoted to uncovering human dishonesty. In the present paper, we review more than a hundred papers from this literature and provide a comprehensive overview by ﬁrst listing the existing theoretical frameworks, and then covering the common empirical approaches, synthesizing the demographic and personal characteristics of those who cheat, identifying the behavioural mechanisms found that affect dishonesty and ﬁnally we ﬁnish by discussing how the empirical evidence ﬁt theory. Overall, the review concludes that many people behave dishonestly, but also that it is a highly malleable behavior sensitive to elements such as decision contexts, behaviour of others, state of mind and depletion. The review can be used as an overview of the dishonesty literature or as a guide or work of reference for selected topics of interest.},
	language = {en},
	number = {2},
	urldate = {2018-10-13},
	journal = {Journal of Economic Surveys},
	author = {Jacobsen, Catrine and Fosgaard, Toke Reinholt and Pascual-Ezama, David},
	month = apr,
	year = {2018},
	pages = {357--387},
	file = {Jacobsen et al. - 2018 - WHY DO WE LIE A PRACTICAL GUIDE TO THE DISHONESTY.pdf:/Users/lukasbosch/Zotero/storage/ICBMLZDJ/Jacobsen et al. - 2018 - WHY DO WE LIE A PRACTICAL GUIDE TO THE DISHONESTY.pdf:application/pdf}
}

